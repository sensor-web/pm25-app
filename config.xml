<?xml version='1.0' encoding='utf-8'?>
<widget id="io.sensorweb.pm25" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PM2.5 app</name>
    <description>
        A simple application to visualize SensorWeb PM2.5 data.
    </description>
    <author email="elin@mozilla.com" href="http://sensorweb.io">
        Eddie Lin
    </author>
    <content src="index.html" />
    <access origin="http://sensorweb.io" subdomains="true"/>
    <access origin="*" />
    <allow-navigation href="http://sensorweb.io/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~5.1.1" />
    <engine name="ios" spec="~4.1.1" />
    <engine name="browser" spec="~4.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-geolocation" spec="~2.2.0" />
    <plugin name="cordova-plugin-dialogs" spec="1" />
    <plugin name="cordova.plugins.diagnostic" spec="~2.3.16" />
    <plugin name="cordova-plugin-network-information" spec="1" />
</widget>
